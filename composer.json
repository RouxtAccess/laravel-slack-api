{
    "name": "vluzrmos/slack-api",
    "description": "Wrapper for Slack.com WEB API.",
    "license": "DBAD",
    "keywords":["Laravel", "Lumen", "Slack"],
    "authors": [
        {
            "name": "Vagner do Carmo",
            "email": "vluzrmos@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "~5.0",
        "illuminate/cache": "~5.0",
        "guzzlehttp/guzzle":"~5.3|~6.0"
    },
    "require-dev": {
      "phpunit/phpunit": "~6.0",
      "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "Vluzrmos\\": "src/Vluzrmos/"
        },
        "files" : [
            "src/Vluzrmos/SlackApi/helpers.php"
        ]
    },
    "extra": {
      "laravel": {
          "providers": [
              "Vluzrmos\\SlackApi\\SlackApiServiceProvider"
          ],
          "aliases": {
              "SlackApi": "Vluzrmos\\SlackApi\\Facades\\SlackApi",
              "SlackChannel": "Vluzrmos\\SlackApi\\Facades\\SlackChannel",
              "SlackChat": "Vluzrmos\\SlackApi\\Facades\\SlackChat",
              "SlackGroup": "Vluzrmos\\SlackApi\\Facades\\SlackGroup",
              "SlackFile": "Vluzrmos\\SlackApi\\Facades\\SlackFile",
              "SlackSearch" : "Vluzrmos\\SlackApi\\Facades\\SlackSearch",
              "SlackInstantMessage": "Vluzrmos\\SlackApi\\Facades\\SlackInstantMessage",
              "SlackUser": "Vluzrmos\\SlackApi\\Facades\\SlackUser",
              "SlackStar": "Vluzrmos\\SlackApi\\Facades\\SlackStar",
              "SlackUserAdmin": "Vluzrmos\\SlackApi\\Facades\\SlackUserAdmin",
              "SlackRealTimeMessage": "Vluzrmos\\SlackApi\\Facades\\SlackRealTimeMessage",
              "SlackTeam": "Vluzrmos\\SlackApi\\Facades\\SlackTeam"
          }
      }
},
    "minimum-stability": "dev",
    "prefer-stable": true
}
